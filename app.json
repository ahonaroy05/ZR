{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to provide stress zone detection and wellness interventions during your commute."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCRdBpyOo4ZZqxE8dn_sBprAgfoROaRskM"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCRdBpyOo4ZZqxE8dn_sBprAgfoROaRskM"
      }
    }
  }
}
